{
    "presets": [["env", {
        "targets": {
            "browsers": ["last 2 versions", "not ie >= 10", "Firefox ESR"]
        },
        "exclude": ["transform-regenerator"],
        "debug": true
    }]],
    "plugins": [
        ["transform-runtime", {
            "helpers": true,
            "polyfill": false,
            "regenerator": false
        }],
        ["module-resolver", {
            "alias": {
                "lodash-es": "lodash"
            }
        }],
        "add-module-exports"]
}