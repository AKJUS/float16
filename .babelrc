{
    "presets": [
        ["@babel/preset-env", {
            "modules": "commonjs",
            "exclude": [
                "transform-regenerator"
            ]
        }]
    ],
    "plugins": [
        ["module-resolver", {
            "alias": {
                "lodash-es": "lodash"
            }
        }],
        ["replace-import-extension", {
            "extMapping": { ".mjs": ".js" }
        }]
    ],
    "env": {
        "test": {
            "sourceMaps": "inline"
        }
    }
}
